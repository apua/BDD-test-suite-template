{
    "info": {
        "name": "Multi SUT S6 JCF4"
    },
    "ckey": {
        "stage1": "ipconfig",
        "stage2": "ls /tmp",
        "stage3": "hostname",
        "target_ip": "10.30.1.20",
        "sys_username": "root",
        "sys_password": "Compaq123"
    },
    "stages":[
        {
            "Run_sys_cmd on Windows": {
                "target":"${sut2}",
                "action": {
                    "cirrus_module": "sys_command",
                    "command": "${ckey.stage1}",
                    "output_mode": "overwrite",
                    "error_file": "/errors.txt",
                    "error_map": {
                        "127": "not found",
                        "1": "not recognized"
                    }
                }
            }
        },
        {
            "Sys cmd on IP (helper system)": {
                "target":"${cirrus}",
                "action": {
                    "cirrus_module": "sys_command",
                    "command": "${ckey.stage2}",
                    "sys_ip":"${ckey.target_ip}",
                    "sys_password":"${ckey.sys_password}",
                    "sys_username":"${ckey.sys_username}",
                    "remote": true
                }
            }
        },
        {
            "Sys cmd on IP (helper system) Again": {
                "target":"${cirrus}",
                "action": {
                    "cirrus_module": "sys_command",
                    "command": "${ckey.stage3}",
                    "sys_ip":"${ckey.target_ip}",
                    "sys_password":"${ckey.sys_password}",
                    "sys_username":"${ckey.sys_username}",
                    "remote": true
                }
            }
        }
    ]
}
