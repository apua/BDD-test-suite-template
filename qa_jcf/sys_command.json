{
    "info": {
        "name": "sys_command"
    },
    "ckey": {
        "command": "ls /tmp"
    },
    "stages":[
        {
            "Run_sys_cmd": {
                "target":"${sut}",
                "action": {
                    "cirrus_module": "sys_command",
                    "command": "${ckey.command}",
                    "output_mode": "overwrite",
                    "error_file": "/errors.txt",
                    "error_map": {
                        "127": "not found",
                        "1": "not recognized"
                    }
                }
            }
        }
    ]
}
