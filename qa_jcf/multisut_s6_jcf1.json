{
    "info": {
        "name": "Multi SUT S6 JCF1"
    },
    "ckey": {
        "stage1": "ifconfig",
        "stage2": "ls /tmp",
        "stage3": "hostname"
    },
    "stages":[
        {
            "Run_sys_cmd on Linux": {
                "target":"${sut}",
                "action": {
                    "cirrus_module": "sys_command",
                    "command": "${ckey.stage1}",
                    "output_mode": "overwrite",
                    "error_file": "/errors.txt",
                    "error_map": {
                        "127": "not found",
                        "1": "not recognized"
                    }
                }
            }
        },
        {
            "Run_sys_cmd on Linux Again": {
                "target":"${sut}",
                "action": {
                    "cirrus_module": "sys_command",
                    "command": "${ckey.stage2}",
                    "output_mode": "overwrite",
                    "error_file": "/errors.txt",
                    "error_map": {
                        "127": "not found",
                        "1": "not recognized"
                    }
                }
            }
        },
        {
            "Run_sys_cmd on Linux Again and Again": {
                "target":"${sut}",
                "action": {
                    "cirrus_module": "sys_command",
                    "command": "${ckey.stage3}",
                    "output_mode": "overwrite",
                    "error_file": "/errors.txt",
                    "error_map": {
                        "127": "not found",
                        "1": "not recognized"
                    }
                }
            }
        }
    ]
}
